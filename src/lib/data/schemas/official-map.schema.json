{
    "$schema": "http://json-schema.org/draft-07/schema",

    "type": "object",
    "required": ["id", "map", "result"],
    "additionalProperties": false,
    "properties": {
        "id": { "type": "number" },
        "map": { "$ref": "../schemas/cs-map.schema.json" },
        "result": { "type": "string", "pattern": "^[0-9]+:[0-9]+$" },
        "links": { "type": "array", "items": { "$ref": "../schemas/extern-link.schema.json" } },
        "rewatches": { "type": "array", "items": { "$ref": "../schemas/official-rewatch.schema.json" } },
        "note": { "type": "string" }
    }
}
